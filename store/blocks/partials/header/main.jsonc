    "children": [
      "image#logo",
      "search-bar",
      "login#desktop",
      "link#wishlist",
      "minicart.v2#new"
    ],
    "props": {
      "blockClass": ["main-container", "desktop"]
    }
  },
  "header-row#main-mobile": {
    "children": ["menu#mobile", "image#logo", "minicart.v2#new"],
    "props": {
      "blockClass": ["main-container"]
    }
  },
  "link#wishlist": {
    "title": "Wishlist",
    "props": {
      "href": "/account/#/wishlist",
      "label": "Meus favoritos",
      "blockClass": "wishlist"
    }
  },
  "image#logo": {
    "title": "Logo da loja",
